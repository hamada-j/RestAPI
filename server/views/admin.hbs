<h2>Admin Settings</h2>
<hr>
<h3>All Admin: {{Response.count }}</h3>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Admin Id</th>
            <th scope="col">Email</th>
            <th scope="col">Password</th>
        </tr>
    </thead>
    <tbody>
        {{#each Response.arrAdmin as |element|}}
        <tr>
            <th scope="row">#</th>
            <th>{{element._id}}</th>
            <td>{{element.email}}</td>
            <td>{{element.password}}</td>
            <td>{{element.description}}</td>
            <td><a href="/admin/category/edit/{{element.id}}" class="btn btn-sm btn-primary">Edit</a></td>
            <td><a href="/admin/category/delete/{{elementR.id}}" class="btn btn-sm btn-danger"
                    onclick="return confirm('Â¿are you ?')">Delete</a></td>
        </tr>
        {{/each}}
    </tbody>
</table>
<hr>
<dir class="card">

    <p>
        <td><a href="/admin/admin" class="btn btn-sm btn-primary">All</a></td>
    </p>
    <h5>{{response}}</h5>
</dir>
<hr>
<div class="col">
    <h2>Category Form</h2>
    <form action="/admin/login/signup/" method="POST">
        <div class="form-group row">
            <label for="email" class="col-sm-2 col-form-label">Admin Email</label>
            <div class="col-sm-10">
                <input name="email" type="text" class="form-control" value="{{element.email}}">
            </div>
        </div>
        <div class="form-group row">
            <label for="password" class="col-sm-2 col-form-label">Category Password</label>
            <div class="col-sm-10">
                <input name="password" type="text" class="form-control" value="{{element.password}}">
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <button type="submit" class="btn btn-primary">Send</button>
            </div>
        </div>
    </form>
</div>